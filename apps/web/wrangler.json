{
    "name": "projects-web",
    "main": "./src/server.ts",
    "compatibility_date": "2024-11-12",
    "compatibility_flags": [
        "nodejs_compat",
        "nodejs_compat_populate_process_env"
    ],
    "assets": {
        "directory": "./dist/client",
        "binding": "ASSETS"
    },
    "vars": {
        "VITE_PUBLIC_SITE_URL": "http://localhost:3002",
        "NODE_ENV": "development"
    },
    "hyperdrive": [
        {
            "binding": "DATABASE",
            "id": "placeholder-create-hyperdrive-first",
            "localConnectionString": "postgresql://postgres:postgres@localhost:5432/projects"
        }
    ],
    "kv_namespaces": [],
    "d1_databases": [],
    "r2_buckets": [
        {
            "binding": "BUCKET",
            "bucket_name": "projects-assets"
        }
    ],
    "ai": {
        "binding": "AI"
    }
}